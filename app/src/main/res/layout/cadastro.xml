<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent" android:background="#5FDCEC">
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/nomeCadastro" android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="60dp"
            android:layout_marginStart="60dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="60dp"
            app:layout_constraintHorizontal_bias="0.0" tools:text="Name" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/emailCadastro" android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/nomeCadastro"
            app:layout_constraintEnd_toEndOf="@+id/nomeCadastro" app:layout_constraintStart_toStartOf="@+id/nomeCadastro"
            app:layout_constraintHorizontal_bias="0.0" tools:text="Email"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Password"
            android:ems="10"
            android:id="@+id/senhaCadastro"
            app:layout_constraintEnd_toEndOf="@+id/emailCadastro" app:layout_constraintStart_toStartOf="@+id/emailCadastro"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/emailCadastro"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText4"
            app:layout_constraintEnd_toEndOf="@+id/senhaCadastro" app:layout_constraintStart_toStartOf="@+id/senhaCadastro"
            tools:text="Confirm Password" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/senhaCadastro" android:layout_marginTop="32dp"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="110dp" app:srcCompat="@drawable/people"
            android:id="@+id/imageView" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="72dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/check"
            android:id="@+id/confirmCategoria" android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/editText4" app:layout_constraintEnd_toEndOf="@+id/editText4"
            android:layout_marginEnd="64dp" android:layout_marginRight="64dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/return"
            android:id="@+id/returnCategoria" app:layout_constraintStart_toStartOf="@+id/editText4"
            android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/editText4" android:layout_marginLeft="64dp"
            android:layout_marginStart="64dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>